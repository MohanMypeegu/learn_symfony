<h1>The list of Students :</h1>

<table>
	<tr>
		<th>Name</th>
		<th>Academic Year</th>
		<th>Gender</th>
		<th>Address</th>
	</tr>
	{% for student in students %}
	<tr>
		<td>{{ student.name|upper }}</td>
		<td>{{ student.academic_year }}</td>
		<td>{{ student.gender }}</td>
		<td>{{ student.address }}</td>
	</tr>
	{% endfor %}
</table>
